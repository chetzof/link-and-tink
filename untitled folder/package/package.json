{
  "name": "linkandtink",
  "version": "1.0.0",
  "description": "oclif example Hello World CLI",
  "author": "chetzof @chetzof",
  "bin": {
    "linktink": "./bin/run"
  },
  "prettier": "chetzof-lint-config/prettier/.prettierrc.js",
  "homepage": "https://github.com/chetzof/linktink",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "chetzof/linktink",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.7",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.0",
    "listr2": "^5.0.6",
    "log-symbols": "4.0.0",
    "node-notifier": "^10.0.1",
    "tsconfig-paths": "^4.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.12",
    "@types/chai": "^4",
    "@types/chokidar": "^2.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.4",
    "@types/node-notifier": "^8.0.2",
    "chai": "^4",
    "chetzof-lint-config": "^1.0.4",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-define-config": "^1.12.0",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.5"
  },
  "oclif": {
    "bin": "linktink",
    "dirname": "linktink",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && tsc-alias",
    "watch": "tsc -w",
    "lint": "eslint . && prettier --write .",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "testÐµ": "mocha --forbid-only \"test/**/*.test.ts\"",
    "vitest": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "bugs": "https://github.com/chetzof/linktink/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
